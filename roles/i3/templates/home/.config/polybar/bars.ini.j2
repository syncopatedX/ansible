[bar]
fill = ⏽
empty = ⏽
indicator =

[module/brightness]
type = internal/backlight
card = intel_backlight
format = <ramp> <bar>
label = %percentage%%
ramp-0 = 
ramp-1 = 
ramp-2 = 
ramp-3 = 
ramp-4 = 
ramp-foreground = {{ colors.red0 }}
bar-format = "%fill%%indicator%%empty%"
bar-width = 11
bar-gradient = false
bar-indicator = ${bar.indicator}
bar-indicator-foreground = {{ colors.red0 }}
bar-indicator-font = 2
bar-fill = ${bar.fill}
bar-fill-font = 2
bar-foreground-0 = {{ colors.red0 }}
bar-foreground-1 = {{ colors.red0 }}
bar-foreground-2 = {{ colors.red0 }}
bar-foreground-3 = {{ colors.red0 }}
bar-foreground-4 = {{ colors.red0 }}
bar-empty = ${bar.empty}
bar-empty-font = 2
bar-empty-foreground = {{ colors.red0 }}

[module/battery_bar]
type = internal/battery
full-at = 99
battery = BAT0
adapter = AC
poll-interval = 2
time-format = %H:%M
format-charging = <bar-capacity>
format-charging-prefix = ""
format-charging-prefix-foreground = {{ colors.red0 }}
format-discharging = <bar-capacity>
format-discharging-prefix = ""
format-discharging-prefix-foreground = {{ colors.red0 }}
format-full = <label-full>
format-full-prefix = ""
format-full-prefix-foreground = {{ colors.red0 }}
label-charging = %percentage%%
label-discharging = %percentage%%
label-full = Full
bar-capacity-format = " %fill%%indicator%%empty%"
bar-capacity-width = 11
bar-capacity-gradient = false
bar-capacity-indicator = ${bar.indicator}
bar-capacity-indicator-font = 2
bar-capacity-indicator-foreground = {{ colors.red0 }}
bar-capacity-fill = ${bar.fill}
bar-capacity-fill-font = 2
bar-capacity-foreground-0 = {{ colors.red0 }}
bar-capacity-foreground-1 = {{ colors.red0 }}
bar-capacity-foreground-2 = {{ colors.red0 }}
bar-capacity-foreground-3 = {{ colors.red0 }}
bar-capacity-foreground-4 = {{ colors.red0 }}
bar-capacity-empty = ${bar.empty}
bar-capacity-empty-font = 2
bar-capacity-empty-foreground = {{ colors.red0 }}
bar-fill-font = 2

[module/cpu_bar]
type = internal/cpu
interval = 0.5
format = <bar-load><label>
format-prefix = 
format-prefix-foreground = {{ colors.red0 }}
label = " %percentage%%"
bar-load-format = " %fill%%indicator%%empty%"
bar-load-width = 11
bar-load-gradient = false
bar-load-indicator = ${bar.indicator}
bar-load-indicator-font = 2
bar-load-indicator-foreground = {{ colors.red0 }}
bar-load-fill = ${bar.fill}
bar-load-fill-font = 2
bar-load-foreground-0 = {{ colors.red0 }}
bar-load-foreground-1 = {{ colors.red0 }}
bar-load-foreground-2 = {{ colors.red0 }}
bar-load-foreground-3 = {{ colors.red0 }}
bar-load-foreground-4 = {{ colors.red0 }}
bar-load-empty = ${bar.empty}
bar-load-empty-font = 2
bar-load-empty-foreground = {{ colors.red0 }}

[module/filesystem_bar]
type = internal/fs
mount-0 = /
interval = 30
fixed-values = false
format-mounted = <bar-used><label-mounted>
format-mounted-prefix = 
format-mounted-prefix-foreground = {{ colors.red0 }}
format-unmounted = <label-unmounted>
format-unmounted-prefix = 
format-unmounted-prefix-foreground = {{ colors.red0 }}
label-mounted = " %used%/%total%"
label-unmounted = " %mountpoint%: not mounted"
bar-used-format = " %fill%%indicator%%empty%"
bar-used-width = 11
bar-used-gradient = false
bar-used-indicator = ${bar.indicator}
bar-used-indicator-font = 2
bar-used-indicator-foreground = {{ colors.red0 }}
bar-used-fill = ${bar.fill}
bar-used-fill-font = 2
bar-used-foreground-0 = {{ colors.red0 }}
bar-used-foreground-1 = {{ colors.red0 }}
bar-used-foreground-2 = {{ colors.red0 }}
bar-used-foreground-3 = {{ colors.red0 }}
bar-used-foreground-4 = {{ colors.red0 }}
bar-used-empty = ${bar.empty}
bar-used-empty-font = 2
bar-used-empty-foreground = {{ colors.red0 }}

[module/memory_bar]
type = internal/memory
interval = 2
format = <bar-used><label>
format-prefix = 
format-prefix-foreground = {{ colors.red0 }}
label = " %mb_used%"
bar-used-format = " %fill%%indicator%%empty%"
bar-used-width = 11
bar-used-gradient = false
bar-used-indicator = ${bar.indicator}
bar-used-indicator-font = 2
bar-used-indicator-foreground = {{ colors.red0 }}
bar-used-fill = ${bar.fill}
bar-used-fill-font = 2
bar-used-foreground-0 = {{ colors.red0 }}
bar-used-foreground-1 = {{ colors.red0 }}
bar-used-foreground-2 = {{ colors.red0 }}
bar-used-foreground-3 = {{ colors.red0 }}
bar-used-foreground-4 = {{ colors.red0 }}
bar-used-empty = ${bar.empty}
bar-used-empty-font = 2
bar-used-empty-foreground = {{ colors.red0 }}

[module/mpd_bar]
type = internal/mpd
interval = 1
format-online = <label-song><bar-progress><label-time>
format-online-prefix = 
format-online-prefix-foreground = {{ colors.red0 }}
format-offline = <label-offline>
format-offline-prefix = 
format-offline-prefix-foreground = {{ colors.red0 }}
label-song =  " %artist% - %title%"
label-song-maxlen = 25
label-song-ellipsis = true
label-time = " %elapsed% / %total%"
label-offline = " Offline"
icon-play = 
icon-pause = 
icon-stop = 
icon-next = 
icon-prev = 
icon-seekf =
icon-seekb =
icon-random =
icon-repeat =
icon-repeatone =
icon-single =
icon-consume =
toggle-on-foreground = {{ colors.red0 }}
toggle-off-foreground = {{ colors.red0 }}
bar-progress-format = " %fill%%indicator%%empty%"
bar-progress-width = 11
bar-progress-gradient = false
bar-progress-indicator = ${bar.indicator}
bar-progress-indicator-font = 2
bar-progress-indicator-foreground = {{ colors.red0 }}
bar-progress-fill = ${bar.fill}
bar-progress-fill-font = 2
bar-progress-foreground-0 = {{ colors.red0 }}
bar-progress-foreground-1 = {{ colors.red0 }}
bar-progress-foreground-2 = {{ colors.red0 }}
bar-progress-foreground-3 = {{ colors.red0 }}
bar-progress-foreground-4 = {{ colors.red0 }}
bar-progress-empty = ${bar.empty}
bar-progress-empty-font = 2
bar-progress-empty-foreground = {{ colors.red0 }}
